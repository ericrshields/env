{
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(pwd)",
      "Bash(cd:*)",
      "Bash(echo:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(which:*)",
      "Bash(wc:*)",
      "Bash(git status)",
      "Bash(git status:*)",
      "Bash(git diff)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git branch:*)",
      "Bash(git show:*)",
      "Bash(git rev-parse:*)",
      "Bash(dpkg -l:*)",
      "Bash(dpkg -L:*)",
      "Bash(apt-cache search:*)",
      "Bash(apt-cache show:*)",
      "Bash(rpm -q:*)",
      "Bash(yum list:*)",
      "Bash(npm:*)",
      "Bash(node:*)",
      "Bash(time:*)",
      "Bash(mkdir -p:*)",
      "Bash(unset:*)",
      "Bash(source:*)",
      "Read(**/*.md)",
      "Read(**/*.txt)",
      "Read(**/*.json)",
      "Read(**/*.pl)",
